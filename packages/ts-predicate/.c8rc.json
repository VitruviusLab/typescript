{
  "src": "./src",
  "exclude": [
    "**/test/**",
	"**/Types/**/*.*",
	"**/index.ts",
	"**/_index.ts"
  ],
  "extension": [
    ".ts"
  ],
  "reporter": [
    "html"
  ],
  "report-dir": "./reports/c8",
  "skip-full": true,
  "check-coverage": true,
  "all": true,
  "statements": 100,
  "branches": 100,
  "functions": 100,
  "lines": 100
}
