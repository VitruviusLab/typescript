{
	"$schema": "https://json.schemastore.org/swcrc.json",
	"jsc": {
	  "parser": {
		"syntax": "typescript",
		"tsx": false,
		"decorators": false,
		"dynamicImport": true
	  },
	  "target": "esnext"
	},
	"module": {
	  "type": "es6",

	  // These are defaults.
	  "strict": false,
	  "strictMode": true,
	  "lazy": false,
	  "noInterop": false
	}
  }
